{
  "name": "@tableau/embedding-api-react",
  "description": "Implementation for Tableau Embedding Api React component",
  "version": "3.13.1",
  "type": "module",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cy:open": "cypress open --component --browser electron",
    "cy:run": "cypress run --component",
    ":ship-release": "node scripts/prepare-and-ship.js",
    ":ship-beta": "node scripts/prepare-and-ship.js --beta",
    ":ship-latest": "node scripts/prepare-and-ship.js --latest"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/embedding-api-react.es.js"
    }
  },
  "module": "./dist/embedding-api-react.es.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@tableau/embedding-api": "~3.13.0",
    "wc-react": "^0.5.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.6",
    "cypress": "^13.15.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.1.0",
    "react-hooks": "^1.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.4",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^3.0.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
